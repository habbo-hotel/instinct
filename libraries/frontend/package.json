{
  "name": "instinct-frontend",
  "version": "4.16.0",
  "description": "Shared components across Instinct Apps",
  "scripts": {
    "build": "rollup -c",
    "deploy:unsafe": "gts clean && npm run build && cp README.md build/README.md && cp package.json build/package.json && cp package-lock.json build/package-lock.json && npm publish build",
    "deploy": "gts clean && npm run test && npm run deploy:unsafe",
    "fix": "gts clean && gts fix",
    "test": "gts clean && tsc && gts check && jest --coverage --forceExit && gts clean",
    "test:update": "",
    "build-storybook": "build-storybook -s public -o public",
    "start": "start-storybook"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "instinct-interfaces": "^1.0.9",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "n": "^6.7.0",
    "react-toastify": "^6.0.9",
    "styled-components": "^5.2.0",
    "tslib": "^2.0.2",
    "wouter": "^2.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.26",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-test-renderer": "^16.9.3",
    "@types/reactstrap": "^8.5.1",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-info": "^5.2.2",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.1.3",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.21.3",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "gts": "^2.0.2",
    "i": "^0.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.2",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "reactstrap": "^8.6.0",
    "rollup": "^2.28.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-bundle-scss": "^0.1.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-typescript2": "^0.27.3",
    "sass-loader": "^10.0.2",
    "thread-loader": "^3.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.3",
    "typescript-styled-plugin": "^0.15.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^4.0.3",
    "npm": "^6.14.8"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "rootDir": "src",
    "setupFiles": [
      "./setupTests.ts"
    ],
    "testEnvironment": "node",
    "transform": {
      ".*.ts": "ts-jest",
      ".*.tsx": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  }
}
