{
  "name": "@instinct-prj/rp-api",
  "version": "0.9.0",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": " forever --sourceDir . -c \"yarn start\" /",
    "update": "yarn add @instinct-prj/backend@latest && yarn update:interface",
    "update:interface": " yarn add @instinct-prj/interface@latest && yarn add @instinct-prj/interface-rp@latest",
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "@instinct-prj/backend": "^1.0.1",
    "@instinct-prj/backend-rp": "^1.0.1",
    "@instinct-prj/cerberus": "^1.0.0",
    "@instinct-prj/interface": "^1.0.1",
    "@instinct-prj/interface-rp": "^1.0.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/typeorm": "^7.1.4",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.29",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "ts-loader": "^8.0.9",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "@types/node": "^14.14.6"
  }
}
