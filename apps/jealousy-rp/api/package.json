{
  "name": "@instinct-prj/jealousy-rp-api",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch",
    "test": "tsc && gts check && jest --coverage",
    "compile": "tsc -p .",
    "fix": "npm run clean && gts fix",
    "build": "tsc -p .",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/typeorm": "^7.1.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "2.29.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.28",
    "@instinct-prj/interface": "0.2.1",
    "@instinct-prj/interface-rp": "0.2.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/express": "^4.17.8",
    "@types/jest": "26.0.14",
    "@types/node": "^14.11.10",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-bundle-scss": "^0.1.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-typescript2": "^0.27.3",
    "sass-loader": "^10.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test|e2e).[tj]s?(x)"
    ],
    "setupFiles": [
      "<rootDir>/setupTests.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
