{
  "name": "instinct-frontend",
  "version": "0.0.4",
  "description": "Shared components across Instinct Apps",
  "scripts": {
    "build": "rollup -c",
    "deploy:unsafe": "gts clean && npm run build && cp README.md build/README.md && cp package.json build/package.json && cp package-lock.json build/package-lock.json && npm publish build",
    "deploy": "gts clean && npm run test && npm run deploy:unsafe",
    "fix": "gts clean && gts fix",
    "test": "gts clean && tsc && gts check && jest --coverage --forceExit && gts clean",
    "test:update": "",
    "build-storybook": "build-storybook -s public -o public",
    "start": "start-storybook"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "instinct-interfaces": "^1.0.4",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0",
    "styled-components": "^5.1.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.33",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/reactstrap": "^8.4.2",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.0.1",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "gts": "^1.1.2",
    "i": "^0.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "npm": "^6.14.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.13.1",
    "reactstrap": "^8.4.1",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-postcss": "^3.1.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "sass-loader": "^8.0.2",
    "thread-loader": "^2.1.3",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "typescript-styled-plugin": "^0.15.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "rootDir": "src",
    "setupFiles": [
      "./setupTests.ts"
    ],
    "testEnvironment": "node",
    "transform": {
      ".*.ts": "ts-jest",
      ".*.tsx": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  }
}
