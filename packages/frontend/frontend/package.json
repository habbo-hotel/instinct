    "rimraf": "^3.0.2",
    "rollup": "^2.33.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass-loader": "^10.0.4",
