    "react-draggable": "^4.4.3",
    "react-google-recaptcha": "^2.1.0",
    "react-loading-overlay": "^1.0.1",
    "react-loading-skeleton": "^2.1.1",
    "react-toastify": "^6.0.9",
    "swfobject": "^2.2.1",
    "wouter": "^2.5.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
