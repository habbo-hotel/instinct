{
  "name": "@instinct-prj/frontend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c && cp README.md build/README.md && cp package.json build/package.json && cp package-lock.json build/package-lock.json",
    "deploy:unsafe": "npm run build && npm publish build",
    "deploy": "npm run test && npm run deploy:unsafe",
    "test": "tsc && jest --coverage --forceExit"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "n": "^6.7.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-datepicker": "^3.3.0",
    "react-draft-wysiwyg": "^1.14.5",
    "react-draggable": "^4.4.3",
    "react-google-recaptcha": "^2.1.0",
    "react-loading-overlay": "^1.0.1",
    "react-loading-skeleton": "^2.1.1",
    "react-toastify": "^6.0.9",
    "styled-components": "^5.2.0",
    "swfobject": "^2.2.1",
    "tslib": "^2.0.3",
    "wouter": "^2.5.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-test-renderer": "^16.9.3",
    "@types/reactstrap": "^8.5.2",
    "@types/styled-components": "^5.1.4",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "i": "^0.3.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.1",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^17.0.1",
    "reactstrap": "^8.6.0",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-bundle-scss": "^0.1.2",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-typescript2": "^0.28.0",
    "sass-loader": "^10.0.4",
    "thread-loader": "^3.0.0",
    "ts-jest": "^26.4.2",
    "ts-loader": "^8.0.6",
    "typescript": "^4.0.3",
    "typescript-styled-plugin": "^0.15.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  },
  "peerDependencies": {
    "npm": "^6.14.8",
    "react": "^17.0.1",
    "typescript": "^4.0.3"
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "rootDir": "src",
    "setupFiles": [
      "./setupTests.ts"
    ],
    "testEnvironment": "node",
    "transform": {
      ".*.ts": "ts-jest",
      ".*.tsx": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  }
}
